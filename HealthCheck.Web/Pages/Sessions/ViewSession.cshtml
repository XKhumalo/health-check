@page
@model HealthCheck.Web.Pages.Sessions.ViewSessionModel
@{
    ViewData["Title"] = "ViewSession";
}

<div class="container">
    <h1 class="h3 mb-3 font-weight-normal">Session Data</h1>
    <form id="join-session-form" class="col-lg-6 col-md-12 col-sm-12" method="post">
        <div class="form-group">
            <label asp-for="SessionViewModel.SessionKey"></label>
            <input asp-for="SessionViewModel.SessionKey" type="text" class="form-control" readonly>
        </div>
        <div class="form-group">
            <label asp-for="SessionViewModel.StartDate"></label>
            <input asp-for="SessionViewModel.StartDate" type="datetime-local" class="form-control" readonly>
        </div>
        <div class="form-group">
            <label asp-for="SessionViewModel.EndTime"></label>
            <input asp-for="SessionViewModel.StartDate" type="datetime-local" class="form-control" readonly>
        </div>
        <div class="form-group">
            <label asp-for="SessionViewModel.DateCreated"></label>
            <input asp-for="SessionViewModel.DateCreated" type="datetime-local" class="form-control" readonly>
        </div>
        <div class="form-group">
            <label asp-for="SessionViewModel.IsComplete"></label>
            <input asp-for="SessionViewModel.IsComplete" type="text" class="form-control" readonly >

        </div>
        <div class="form-group">
            <label asp-for="SessionViewModel.Categories"></label>
            <input asp-for="SessionViewModel.Categories" type="text" class="form-control" readonly>
        </div>
        <button asp-page-handler="create" class="btn btn-lg btn-primary btn-block" type="submit">Continue</button>
    </form>
